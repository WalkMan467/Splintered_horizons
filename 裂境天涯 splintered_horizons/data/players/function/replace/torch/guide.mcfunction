tag @s remove player.replace.torch
playsound minecraft:entity.item.pickup voice @s ~ ~1 ~ 0.5 0.75

scoreboard players set #player.replace.torch.temp global.main 0


execute if items entity @s container.1 #torchs run item replace entity @s weapon.mainhand from entity @s container.1
execute if items entity @s container.1 #torchs run scoreboard players set #player.replace.torch.temp global.main 1
execute if items entity @s container.1 #torchs run item replace entity @s container.1 with air

execute if score #player.replace.torch.temp global.main matches 1 run return 0


